source "https://rubygems.org"

# GitHub Pages gem ensures all compatible versions
gem "github-pages", group: :jekyll_plugins

# Additional plugins you use. Grouping them under :jekyll_plugins ensures
# Bundler installs them along with the GitHub Pages environment.
group :jekyll_plugins do
	gem "jekyll-sitemap"
	gem "jekyll-feed"
	gem "jekyll-seo-tag"
	gem "jekyll-paginate-v2"
	gem "jekyll-scholar"
	gem "jekyll-email-protect"
	gem "jekyll-twitter-plugin"
	gem "jemoji"
	gem "jekyll-diagrams"
end

